        "module": "commonjs",
        "moduleResolution": "node",
        "strict": true,
        "target": "es2017",
        "sourceMap": true,
        "inlineSourceMap": true,
        "inlineSources": true
    },
    "plugins": [
        {
            "name": "tslint-language-service",
            "ignoreDefinitionFiles": true
        }
    ]
}
